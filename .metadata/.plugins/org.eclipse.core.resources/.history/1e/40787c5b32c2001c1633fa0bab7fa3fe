package com.capg.Customer;

import java.util.ArrayList;
import java.util.List;


public class Customer {
	
	String name;
	int numberOfRooms;
	
	public Customer(String name, int numberOfRooms) {
		this.name = name;
		this.numberOfRooms = numberOfRooms;
	}
}

class Hotel{
	
	List<String> customerName=new ArrayList<String>();
	
	int roomsBooked=0;
	String check1="Checked in";
	String check2="No rooms available ";
	String check3="Yes";
	String check4="No";
	
	public String checkIn(Customer customer) {
		if(roomsBooked<10) {
			return check1;
		}
		else 
			return check2;
	}
	
	public String checkRooms(Customer customer) {
		if(roomsBooked<10) {
			return check3;
		}
		else
			return check4;
		}
 	}


	

